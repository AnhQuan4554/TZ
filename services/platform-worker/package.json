{
  "name": "@tymlez/platform-worker",
  "version": "0.0.1",
  "description": "Platform worker",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rm -rf dist coverage",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:nodemon": "nodemon dist/main",
    "start:dev": "concurrently \"yarn watch\" \"yarn start:nodemon\"",
    "watch": "yarn build -w",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "deploy": "tymlez-cli deploy backend --type worker",
    "deploy:cohort:prod": "tymlez-cli deploy backend --type worker"
  },
  "dependencies": {
    "@bull-board/api": "^4.2.2",
    "@bull-board/express": "^4.2.2",
    "@mikro-orm/core": "^5.6.15",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/microservices": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@tymlez/backend-libs": "*",
    "@tymlez/common-libs": "*",
    "axios": "^1.2.3",
    "bull": "^4.10.2",
    "concurrently": "^7.6.0",
    "cron": "^2.1.0",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "lodash": "^4.17.21",
    "nats": "^2.11.0",
    "nestjs-pino": "^3.1.2",
    "promise-retry": "^2.0.1",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@tymlez/backend-libs": "*",
    "@tymlez/common-libs": "*",
    "@types/bull": "^3.15.9",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.15",
    "@types/jest": "28.1.8",
    "@types/node": "^16.18.11",
    "@types/promise-retry": "^1.1.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.9.4"
  }
}
