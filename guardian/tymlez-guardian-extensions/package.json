{
  "name": "@tymlez/tymlez-guardian-extensions",
  "version": "0.0.1",
  "description": "The microservice to add extensions into guardian",
  "author": "development@tymlez.com",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:nodemon": "nodemon dist/main",
    "start:dev": "concurrently \"yarn watch\" \"yarn start:nodemon\"",
    "watch": "tsc -w",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "deploy:guardian": "tymlez-cli deploy gke --imageName tymlez-guardian-extensions",
    "deploy:cohort:prod": "tymlez-cli deploy gke --chart tymlez-guardian-extensions"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.9.0",
    "@mikro-orm/core": "^5.6.15",
    "@mikro-orm/mongodb": "^5.6.15",
    "@mikro-orm/nestjs": "^5.1.7",
    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/microservices": "^9.3.9",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/terminus": "^9.2.1",
    "@tymlez/backend-libs": "*",
    "@tymlez/common-libs": "*",
    "axios": "^1.2.3",
    "lodash": "^4.17.21",
    "nats": "^2.11.0",
    "nestjs-pino": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/express": "^4.17.15",
    "@types/jest": "28.1.8",
    "@types/lodash": "^4.14.191",
    "@types/node": "^16.18.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "concurrently": "^7.6.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.4"
  }
}
